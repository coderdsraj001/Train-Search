<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th><h1>Station name</h1></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stations.each do |station| %>
      <tr>
        <td><%= link_to station.name, station, class: "btn btn-outline-secondary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>



